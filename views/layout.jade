        script(type='text/javascript').
          function verHijasFondos(idCategoria)
          {
          var divItemActivo = document.getElementById("div-item"+idCategoria);
          var divItemHijosActivo = document.getElementById("div-item-hijos"+idCategoria);
          var centinelaCategoriaDesactivar = document.getElementById("idCategoriaActiva");
          var divItemDesactivar = document.getElementById("div-item"+centinelaCategoriaDesactivar.value);
          var divItemHijosDesactivar = document.getElementById("div-item-hijos"+centinelaCategoriaDesactivar.value);
          divItemHijosDesactivar.className = "div-item-hijos-ocultar";
          divItemDesactivar.className = "div-item";
          divItemActivo.className = "div-item-activo";
          divItemHijosActivo.className = "div-item-hijos";
          centinelaCategoriaDesactivar.value = idCategoria;
          }
          function openOptionsServices()
          {
          var divOptions = document.getElementById('divOptionsServices');
          var spanVineteService = document.getElementById('span_vinete');
          var icon_right = document.getElementById('icon_flecha_right');
          var icon_bottom = document.getElementById('icon_flecha_bottom');
          if ((divOptions.style.display=='none' || divOptions.style.display==''))
          {
          divOptions.style.display = 'block';
          spanVineteService.className = "vinete_menu_active";
          icon_right.style.display = 'none';
          icon_bottom.style.display = 'block';
          }
          else
          {
          divOptions.style.display = 'none';
          spanVineteService.className = "vinete_menu";
          icon_bottom.style.display = 'none';
          icon_right.style.display = 'block';
          }
          }
